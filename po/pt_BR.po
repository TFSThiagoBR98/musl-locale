#: ../locale.c:52
#, c-format
msgid ""
"Usage: %s [-a | -m] [FORMAT] name...\n"
"\n"
"\t-a, --all-locales\tWrite names of all available locales\n"
"\t-m, --charmaps\tWrite names of available charmaps\n"
"\n"
"FORMAT:\n"
"\t-c, --category-name\tWrite names of selected categories\n"
"\t-k, --keyword-name\tWrite names of selected keywords\n"
msgstr ""
"Uso: %s [-a | -m] [FORMATO] nome...\n"
"\n"
"\t-a, --all-locales\tEscreve os nomes de todos as linguas disponíveis\n"
"\t-m, --charmaps\tEscreve os nomes de todos os mapas de caracteres\n"
"\n"
"FORMATO:\n"
"\t-c, --category-name\tEscreve os nome das caregorias selecionadas\n"
"\t-k, --keyword-name\tEscreve os nomes de todas as palavras chaves\n"

